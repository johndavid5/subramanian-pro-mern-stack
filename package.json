  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon -w dist dist/server.js",
    "start-hook": "nodemon -w server server/start_hook.js",
    "compile-server": "babel server --presets node8 --out-dir dist --source-maps",
    "watch-server": "babel server --presets node8 --out-dir dist --source-maps --watch",
    "compile": "webpack",
    "watch": "webpack-dev-server --hot --inline",
    "uwatch": "webpack --watch",
    "twatch": "webpack --watch | perl ./scripts/tail.pl",
	"dev-all": "npm run watch & npm run watch-server & npm start",
	"dev-all-hook": "npm run watch & npm run start-hook",
    "ls": "npm ls --depth=0",
    "ls-tree": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johndavid5/subramanian-pro-mern-stack.git"
