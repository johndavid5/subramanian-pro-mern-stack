{
  "name": "pro-mern-stack",
  "version": "1.0.0",
  "description": "https://github.com/vasansr/pro-mern-stack",
  "main": "server/server.js",
  "scripts": {
    "watch": "webpack-dev-server --hot --inline",
    "watch-server": "webpack --config webpack.server-config.js --watch",
    "start": "node dist/server.bundle.js",
    "dev-all": "npm run watch & npm run watch-server & npm start",
    "compile-server": "webpack --config webpack.server-config.js",
    "start-hook": "nodemon -e js,jsx -w server -w src server/start_hook.js",
    "compile": "webpack",
    "dev-all-hook": "npm run watch & npm run start-hook",
    "": "",
    "start-nodemon": "nodemon -w dist dist/server.bundle.js",
    "uwatch": "webpack --watch",
    "twatch": "webpack --watch | perl ./scripts/tail.pl",
    "test": "mocha --watch --recursive",
    "bootstrap-copy": "cp -r ./node_modules/bootstrap/dist ./static/bootstrap",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive",
    "lint": "eslint.cmd --ext jsx,js src server webpack.config.js",
    "b-lint": "scripts\\b-eslint.bat",
    "ls": "npm ls --depth=0",
    "ls-tree": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johndavid5/subramanian-pro-mern-stack.git"
  },
  "keywords": [
    "mern",
    "react",
    "mongodb",
    "express",
    "node"
  ],
  "author": "John D. Aynedjian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johndavid5/subramanian-pro-mern-stack/issues"
  },
  "homepage": "https://github.com/johndavid5/subramanian-pro-mern-stack#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "co": "^4.6.0",
    "express": "^4.16.1",
    "log4js": "^2.3.10",
    "mongodb": "^2.2.33",
    "object-assign": "^4.1.0",
    "prop-types": "^15.6.0",
    "react": "^15.3.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "react-router-bootstrap": "^0.23.1",
    "serve-favicon": "^2.4.5",
    "source-map-support": "^0.5.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "co": "^4.6.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.1",
    "nodemon": "^1.12.1",
    "react-router-dom": "^4.2.2",
    "sinon": "^1.17.5",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
